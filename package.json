    "autoprefixer": "^10.4.12",
    "eslint-plugin-react-hooks": "^4.6.0",
    "node-sass": "^7.0.3",
    "postcss": "^8.4.17",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sass": "^1.12.16",
